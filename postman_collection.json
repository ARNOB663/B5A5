              "body": "{\"success\":true,\"message\":\"Users retrieved successfully\",\"data\":{\"users\":[{\"_id\":\"64f0f3b3a1e2c0b8d6e4f7a1\",\"name\":\"Test User\",\"email\":\"test@example.com\",\"phone\":\"+1234567890\"}] ,\"pagination\":{\"currentPage\":1,\"totalPages\":1,\"totalUsers\":1}}}"
            }
